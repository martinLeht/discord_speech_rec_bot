  "dependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.5",
    "cross-env": "^7.0.3",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "inversify": "^5.1.1",
